<?xml version="1.0"?>
<root>
  <item>
    <name>Swap Space and Quote</name>
    <identifier>private.swap_space_and_quote</identifier>
    <autogen>__KeyToKey__ KeyCode::SPACE, KeyCode::QUOTE</autogen>
    <autogen>__KeyToKey__ KeyCode::QUOTE, KeyCode::SPACE</autogen>
  </item>

  <item>
    <name>Change Right Command Key to QUOTE</name>
    <identifier>private.right_command_to_quote</identifier>
    <autogen>__KeyToKey__ KeyCode::COMMAND_R, KeyCode::QUOTE</autogen>
  </item>

  <item>
    <name>Change Left Command Key to TAB</name>
    <identifier>private.left_command_to_tab</identifier>
    <autogen>__KeyToKey__ KeyCode::COMMAND_L, KeyCode::TAB</autogen>
  </item>
</root>

